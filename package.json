{
  "name": "@kurocado-studio/styleguide-npm-template",
  "version": "0.0.0-semantically-released",
  "description": "",
  "keywords": [],
  "author": "@kurocado-studio",
  "license": "MIT",
  "main": "dist/index.js",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "type": "module",
  "scripts": {
    "build": "rm -rf dist && tsup --dts",
    "commitlint": "pnpm exec commitlint --edit .git/COMMIT_EDITMSG",
    "eslint": "eslint --max-warnings=0 .",
    "eslint-check": "eslint --max-warnings=0 .",
    "eslint-fix": "eslint --max-warnings=0 . --fix",
    "lint": "eslint --max-warnings=0 .",
    "prepare": "husky install",
    "prettier-check": "prettier --check . ",
    "prettier-fix": "prettier --check . --write",
    "semantic-release": "semantic-release",
    "test": "vitest run --coverage",
    "typecheck": "tsc"
  },
  "lint-staged": {
    "*": [
      "pnpm run prettier-fix"
    ],
    "**/*.{ts,tsx}": [
      "pnpm run eslint-fix",
      "pnpm run lint"
    ]
  },
  "devDependencies": {
    "@commitlint/cli": "^19.0.3",
    "@commitlint/config-conventional": "^19.0.3",
    "@kurocado-studio/qa": "1.0.0",
    "@kurocado-studio/styleguide": "2.2.4",
    "@testing-library/jest-dom": "^6.6.3",
    "@vitejs/plugin-react": "^4.3.3",
    "@vitest/coverage-v8": "^3.0.5",
    "eslint": "^8.57.0",
    "husky": "^9.0.11",
    "lint-staged": "^15.2.10",
    "prettier": "^3.3.3",
    "semantic-release": "^23.0.2",
    "tsup": "^8.3.6",
    "vitest": "^3.0.5"
  },
  "publishConfig": {
    "access": "public"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    }
  }
}
